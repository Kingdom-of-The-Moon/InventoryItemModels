{
  "schemaVersion": 1,
  "id": "invmodels",
  "version": "${version}",

  "name": "Inventory Item Models",
  "description": "Allows resource pack creators to override item models in certain situations, like spyglasses and tridents do.",
  "authors": [
    "omoflop"
  ],
  "contact": {
    "sources": "https://github.com/Kingdom-of-Moon/InventoryItemModels"
  },

  "license": "CC0-1.0",
  "icon": "assets/invmodels/icon.png",

  "environment": "client",
  "entrypoints": {
    "client": [
      "org.moon.invmodels.InvModelsMod"
    ]
  },
  "mixins": [
    "invmodels.mixins.json"
  ],

  "depends": {
    "fabricloader": ">=0.11.3",
    "fabric": "*",
    "minecraft": ">=1.17",
    "java": ">=16"
  }
}
